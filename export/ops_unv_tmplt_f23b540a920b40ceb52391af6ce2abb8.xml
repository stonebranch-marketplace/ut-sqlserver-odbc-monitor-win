#
#    Name: odbc_database_monitor.py
#
#    Purpose: monitors a value in a database column
#
#
#    Prerequisites:
#     - Universal Agent for Windows/Linux
#     - Python 2.7.x
#     - ODBC driver installed
#     - Requires the Python modules via pip
#     - pip install pyodbc
#     - pip install datetime
#     - pip install argparse
#     - pip install logging
#
#     Version History:
#     1.0  Nils Buer    2018-03-14  initial version
#     
#          
####################################################

import pyodbc
import argparse
import sys
import datetime
import os 
import time
import logging
# 

# -- logging settings DEBUG, INFO, WARNING, ERROR, CRITICAL

logging.basicConfig(level=logging.${ops_var_loglevel}, format=' %(asctime)s - %(levelname)s - %(message)s')

# -- Main Logic Function
def main():
    """calls the required functions to run the informatica workflow"""
    PrintVariables()       # -- Set Variables from Universal Task Form or Command Line Input
    monitor()            # -- Monitor database table         

def PrintVariables():
   logging.info(" ## Connection settings:")
   logging.info(" Server: ${ops_var_server}")
   logging.info(" database: ${ops_var_database}")
   logging.info(" username: ${_credentialUser('${ops_var_credentials}")
   logging.info(" password: ${_credentialPwd('${ops_var_credentials}')}")
   logging.info(" schema: ${ops_var_schema}")
   logging.info(" table: ${ops_var_table}")
   logging.info(" column: ${ops_var_column}")
   logging.info(" value: ${ops_var_value}")
   logging.info(" andcondition: ${ops_var_andcondition}")
   
def monitor():
   cnxn = pyodbc.connect('DRIVER={Sql Server};SERVER=${ops_var_server};DATABASE=${ops_var_database};UID=${_credentialUser('${ops_var_credentials}')};PWD=${_credentialPwd('${ops_var_credentials}')}')
   # cnxn = pyodbc.connect('DRIVER={Sql Server};SERVER='+server+';DATABASE='+database+';UID='+username+';PWD='+ password)
   cursor = cnxn.cursor()

   found = False # allow for at least one iteration
   while not found:
     cursor.execute("select * from [${ops_var_database}].[${ops_var_schema}].[${ops_var_table}] where [${ops_var_column}] like '${ops_var_value}' ${ops_var_andcondition}")

     row = cursor.fetchone()
     if row:
        logging.debug(" -- Row Found: " + str(row))
        found = True
     else:
        logging.debug(" -- Row not Found")
        time.sleep(${ops_var_interval})

# -- 

